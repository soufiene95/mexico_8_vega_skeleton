{
    "name": "vega/skeleton",
    "description": "Learn poo in php building a traditional web framework",
    "type": "project",
    "license": "MIT",
    "keywords": ["php8", "dwwm", "nextformation", "mexico", "poo", "web","framework", "vega"],
    "homepage": "https://github.com/soufiene95/mexico_8_vega_skeleton",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "VegaCore\\": "vega_core/"
        }
    },
    "authors": [
        {
            "name": "soufiene",
            "email": "soufiene95@gmail.com",
            "homepage": "https://github.com/soufiene95",
            "role": "developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require": {
        "eredost/attributes-router": "^1.0",
        "monolog/monolog": "^3.5",
        "php-di/php-di": "^7.0",
        "symfony/dotenv": "^6.3",
        "symfony/http-foundation": "^6.3",
        "twig/twig": "^3.8"
    },
    "require-dev": {
        "filp/whoops": "^2.15",
        "symfony/var-dumper": "^6.3"
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/Eredost/attributes-router.git"
        }
    ]
}
